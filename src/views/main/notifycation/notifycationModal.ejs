<div class="modal fade" id="notifycationsModal" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Tất cả thông báo</h4>
            </div>
            <div class="modal-body">
                <div class="mark-notify-as-read">
                    <a href="javascript:void(0)" id="modal-mark-notify-as-read">
                        <strong>Đánh dấu tất cả đã đọc</strong>
                    </a>
                </div>
                <hr>
                <ul class="list-notifications">
                    <% notifycations.forEach(function(notifications){ %>
                        <li>
                            <%- notifications %>
                        </li>
                        <% }) %>
                </ul>
                <hr>
                <div class="read-more-notify">
                    <a href="javascript:void(0)" id="link-read-more-notify">
                        <strong>Xem thêm</strong>
                    </a>
                    <div class="lds-hourglass"></div>
                </div>
                <hr>
            </div>
        </div>
    </div>
</div>