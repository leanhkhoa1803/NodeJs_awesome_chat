<!DOCTYPE html>
<html lang="en">

<head>
    <title>Awesome Chat</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="shortcut icon" href="images/logo_favicon/favicon-trungquandev.png" />
    <link rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" href="bower_components/font-awesome/css/font-awesome.css">
    <link rel="stylesheet" href="bower_components/AlertifyJS/build/css/alertify.css">
    <link rel="stylesheet" href="bower_components/emojionearea/dist/emojionearea.min.css">
    <link rel="stylesheet" href="bower_components/photoset-grid/css/colorbox.css">
    <script src="js/authConfig.js"></script>

    <link rel="stylesheet" href="css/style.css">
</head>

<body onload="spinLoaded()">
    <!--Icon loader-->
    <div id="loader"></div>
    <input type="hidden" id="ice-server-list" value="<%= iceServerList %>">

    <%- include('alert/alert'); %>
        <%- include('navbar/navbar'); %>
            <%- content %>

                <div id="img-chat-modal" class="modal">
                    <span class="close">&times;</span>
                    <img class="modal-content" id="img-chat-modal-content">
                    <div id="caption"></div>
                </div>
                <!--Libraries-->
                <script src="bower_components/jquery/dist/jquery.min.js"></script>
                <script src="bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
                <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.1.1/socket.io.js"></script>
                <script src="bower_components/jquery.nicescroll/jquery.nicescroll.min.js"></script>
                <script src="bower_components/AlertifyJS/build/alertify.js"></script>
                <script src="bower_components/moment/min/moment.min.js"></script>
                <script src="bower_components/moment/min/moment-with-locales.min.js"></script>
                <script src="bower_components/emojionearea/dist/emojionearea.min.js"></script>
                <script src="bower_components/peerjs/peerjs.min.js"></script>
                <script src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js"></script>
                <script src="bower_components/sweetalert2/dist/sweetalert2.all.min.js"></script>
                <script src="bower_components/photoset-grid/jquery.photoset-grid.min.js"></script>
                <script src="bower_components/jquery-colorbox/jquery.colorbox-min.js"></script>
                <script src="//cdn.jsdelivr.net/npm/sweetalert2@10"></script>
                <script src="socket.io/socket.io.js"></script>
                <!-- My js-->
                <script src="js/mainConfig.js"></script>
                <script src="js/updateUser.js"></script>

                <script src="js/caculateNotifyContact.js"></script>
                <script src="js/caculateNotifyCation.js"></script>

                <script src="js/addContact.js"></script>
                <script src="js/removeContact.js"></script>

                <script src="js/removeRequestContactSent.js"></script>
                <script src="js/removeRequestContactReceived.js"></script>

                <script src="js/acceptRequestContactReceived.js"></script>

                <script src="js/findUserContact.js"></script>
                <script src="js/readMoreContacts.js"></script>
                <script src="js/readMoreContactsSent.js"></script>
                <script src="js/readMoreContactsReceived.js"></script>
                <script src="js/readMoreNotify.js"></script>
                <script src="js/markNotifyAsRead.js"></script>

                <script src="js/textAndEmojiChat.js"></script>
                <script src="js/caculateNumberMessageGroup.js"></script>
                <script src="js/typingOnOff.js"></script>
                <script src="js/imageChat.js"></script>
                <script src="js/attachmentChat.js"></script>
                <script src="js/videoChat.js"></script>

                <script src="js/checkUserOnline.js"></script>
                <script src="js/createGroupChat.js"></script>

                <script src="js/readMoreAllChat.js"></script>
                <script src="js/readMoreUserChat.js"></script>
                <script src="js/readMoreGroupChat.js"></script>
                <script src="js/readMoreMessage.js"></script>
                <script src="js/searchConversation.js"></script>
                <%- include('contact/contactModal'); %>
                    <%- include('streaming/streamingModal'); %>
                        <%- include('image/imagesModal'); %>
                            <%- include('attachment/attachmentModal'); %>
                                <%- include('user/userModal'); %>
                                    <%- include('groupChat/groupChatModal'); %>
                                        <%- include('notifycation/notifycationModal'); %>

                                            <!-- extras -->
                                            <%- include('extras/membersModal'); %>

</body>

</html>