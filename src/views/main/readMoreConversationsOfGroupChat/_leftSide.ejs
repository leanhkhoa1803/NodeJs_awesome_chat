<% newAllConversations.forEach(function(conversation){ %>
    <% if(conversation.members){ %>
        <a href="#uid_<%= conversation._id %>" class="room-chat" data-target="#to_<%= conversation._id %>">
            <li class="person group-chat" data-chat="<%= conversation._id %>">
                <div class="left-avatar">
                    <!-- <div class="dot"></div> -->
                    <img src="images/users/group-avatar-trungquandev.png" alt="">
                </div>
                <span class="name">
                    <span class="group-chat-name">
                        <% if(conversation.name.length> 15){ %>
                            <%= conversation.name.trim().substr(0,14) %><span>...</span>
                                <% }else{ %>
                                    <%= conversation.name.trim() %>
                                        <% }; %>
                    </span>
                </span>
                <span class="time">
                    <%= covertTimeStamp(getLastItemOfArray(conversation.messages).createdAt) %>
                </span>
                <span class="preview">
                    <% if(getLastItemOfArray(conversation.messages).messageType==="text" ){ %>
                        <%= getLastItemOfArray(conversation.messages).text %>
                            <% } %>
                                <% if(getLastItemOfArray(conversation.messages).messageType==="image" ){ %>
                                    Hình ảnh ...
                                    <% } %>
                                        <% if(getLastItemOfArray(conversation.messages).messageType==="file" ){ %>
                                            Tiệp đính kèm
                                            <% } %>
                </span>
            </li>
        </a>
        <% } %>
            <% }); %>